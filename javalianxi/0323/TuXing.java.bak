public class TuXing{
	public static void main(String[] args){
	    int i = 1;
		int j = 1;
		while (i < 7){
			while(j <= i){
				System.out.print("*");
				j++;
			}
			j = 1;
			System.out.print("\n");
			i++;
		}

		int m = 8;
		int n = 8;
		while(m > 1){
			while(n  m){
				System.out.print("*");
				n--;
			}
			n = 8 ;
			System.out.print("\n");
			m--;
		}
	}
}
