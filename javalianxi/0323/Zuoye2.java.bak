public class Zuoye2{
	public static void main(String[] args){
	    int i,j,k;
		
		int t = 0;
		for (i = 1; i <= 4 ;){
			for (j = 1; j <= 4 ; ){
				for (k = 1; k <= 4 ; ){
					if (i != j && j != k && i != k){
						t += 1;
						System.out.println(i * 100 + j * 10 + k);
					}
				}k++;
			}j++;
		}i++;
		System.out.println("总个数为" + t);
	}
}