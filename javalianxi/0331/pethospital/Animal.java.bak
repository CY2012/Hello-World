package pethospital;

public class Animal{
	private String name;

	public Animal(){}

	public Animal(String name){
		this.name = name;
	}
	
	public void SetName(String name){
		this.name = name;
	}
	public String getName(){
		return name;
	}

	
	public void makeNoise(){
		System.out.println("什么动物发声");
	
	}

}